
server {
    listen 8001;
    server_name localhost;
    error_log    stderr warn;
    access_log   /dev/stdout main;
    location / {
        include      /etc/nginx/uwsgi_params;
        uwsgi_pass   http://localhost:8000;
    }
    location /static/ {
        alias /code/cryptoclient/static/;
    }
    location /media/ {
        alias /code/cryptoclient/media/;
    }
}